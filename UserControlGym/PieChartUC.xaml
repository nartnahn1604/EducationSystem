<UserControl x:Class="IT008_UIT.UserControlGym.PieChartUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IT008_UIT.UserControlGym"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:vms="clr-namespace:IT008_UIT.ViewModel">
    <UserControl.DataContext>
        <vms:PieChartViewModel></vms:PieChartViewModel>
    </UserControl.DataContext>
    <lvc:PieChart Series="{Binding Series}" LegendPosition="Right"/>
</UserControl>